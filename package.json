{
  "name": "rizzdown-workspace",
  "private": true,
  "repository": "https://github.com/benallfree/rizzdown",
  "homepage": "https://github.com/benallfree/rizzdown#readme",
  "author": "benallfree (https://github.com/benallfree)",
  "bin": {
    "rizzdown": "./packages/cli/src/index.ts"
  },
  "version": "0.0.4",
  "license": "MIT",
  "scripts": {},
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "prettier": {
    "semi": false,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "./node_modules/prettier-plugin-organize-imports/index.js",
      "./node_modules/prettier-plugin-svelte/plugin.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@parcel/packager-ts": "2.9.3",
    "@parcel/transformer-typescript-types": "2.9.3",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-svelte": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
